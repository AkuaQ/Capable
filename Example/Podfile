pod 'Capable', :path => '../'

target 'Example-iOS' do
  platform :ios, '10.0'
  use_frameworks!

  pod 'AppCenter/Analytics'
  pod 'Fabric'
  pod 'Answers'
  pod 'Firebase/Core'
end

target 'Example-tvOS' do
  platform :tvos, '10.0'
  use_frameworks!
end

target 'Example-watchOS' do
  platform :watchos, '4.0'
  use_frameworks!
end

target 'Example-watchOS Extension' do
  platform :watchos, '4.0'
  use_frameworks!
end
